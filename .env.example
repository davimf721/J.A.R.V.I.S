# ==================== OLLAMA ====================
OLLAMA_URL=http://ollama:11435/api/generate
OLLAMA_MODEL=kimi-k2.5:cloud
LLM_TIMEOUT=300

# ==================== POSTGRESQL ====================
POSTGRES_USER=jarvis
POSTGRES_PASSWORD=jarvis_secure_password
POSTGRES_DB=jarvis_db
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# ==================== REDIS ====================
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# ==================== RABBITMQ ====================
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=jarvis
RABBITMQ_PASSWORD=jarvis_queue_pwd
RABBITMQ_VHOST=jarvis

# ==================== STORAGE (S3/MINIO) ====================
S3_ENDPOINT=http://minio:9000
S3_BUCKET=jarvis-media
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin

# ==================== CHROMADB ====================
CHROMADB_HOST=chromadb
CHROMADB_PORT=8000
CHROMADB_PERSIST_DIR=/data/chromadb

# ==================== API ====================
API_TITLE=JARVIS AI Platform
API_VERSION=1.0.0
SECRET_KEY=your-secret-key-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30
ENABLE_AUTH=true
ENABLE_RATE_LIMITING=true
ENABLE_TELEMETRY=false

# ==================== LOGGING ====================
LOG_LEVEL=INFO

# ==================== GRAFANA ====================
GRAFANA_PASSWORD=admin
